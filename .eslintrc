{
  "extends": "ndla",
  "env": {
    "jest": true
  },
  "rules": {
    "react/no-deprecated": 0,
    "react/forbid-prop-types": 0,
    "react/prop-types": [
      2,
      {
        "ignore": [
          "children",
          "className",
          "t"
        ]
      }
    ],
    "react/no-did-mount-set-state": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "to"
        ]
      }
    ]
  }
}
