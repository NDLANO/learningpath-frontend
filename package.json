    "eslint": "^3.13.1",
    "express": "4.14.0",
    "express-request-proxy": "2.0.0",
    "history": "3.2.0",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "ndla-learningpath-styleguide": "0.1.37",
    "ndla-licenses": "0.0.6",
    "ndla-ui": "^0.1.6",
    "node-polyglot": "2.2.2",
    "node-uuid": "1.4.7",
    "query-string": "4.3.1",
    "react": "15.4.2",
    "react-bem-helper": "^1.2.0",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
