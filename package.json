    "btoa": "1.2.1",
    "bunyan": "1.8.12",
    "classnames": "2.2.6",
    "compression": "1.7.3",
    "date-fns": "^1.29.0",
    "defined": "1.0.0",
    "draft-convert": "^2.1.4",
    "draft-js": "^0.10.5",
    "eslint": "^5.2.0",
    "express": "^4.16.3",
