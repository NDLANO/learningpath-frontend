{
  "name": "learningpath-frontend",
  "version": "0.1.16",
  "description": "NDLA Learning Path Frontend",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "NODE_ENV=unittest babel-tape-runner 'src/**/*-test.js'|tap-spec",
    "tdd": "watch 'NODE_ENV=unittest babel-tape-runner 'src/**/*-test.js'|tap-spec||true' src/",
    "copy-svg-polyfill": "mkdir -p htdocs/assets && cp node_modules/svg4everybody/dist/svg4everybody.min.js htdocs/assets/",
    "prebuild": "npm run clean && npm run copy-svg-polyfill",
    "build": "NODE_ENV=production webpack --config ./webpack.config.prod.js -p --bail",
    "start": "NODE_ENV=development nodemon --watch server server/server.js",
    "start-prod": "NODE_ENV=production node server/server.js",
    "lint": "eslint src server"
  },
  "engines": {
    "node": "~6.2.2",
    "npm": "~3.9.5"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-tape-runner": "2.0.1",
    "css-loader": "0.26.1",
    "enzyme": "2.7.0",
    "eslint-config-ndla": "0.0.7",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "flux-standard-action": "1.0.0",
    "jsdom": "9.9.1",
    "json-loader": "0.5.4",
    "nock": "9.0.2",
    "nodemon": "1.11.0",
    "react-addons-test-utils": "15.4.2",
    "redux-mock-store": "1.1.4",
    "rimraf": "2.5.4",
    "sinon": "1.17.7",
    "style-loader": "0.13.1",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "watch": "1.0.1",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "babel-register": "6.18.0",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "date-fns": "1.26.0",
    "defined": "1.0.0",
    "draft-js": "0.9.1",
