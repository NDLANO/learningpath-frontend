{
  "name": "learningpath-frontend",
  "version": "0.1.19",
  "description": "NDLA Learning Path Frontend",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider razzle build --noninteractive",
    "postbuild": "cp build/assets.json build/public/assets.json",
    "check-all": "yarn lint && yarn test-ci",
    "e2e": "cross-env NODE_ENV=development cypress open",
    "e2e-use-fixtures": "cross-env NODE_ENV=development CYPRESS_USE_FIXTURES=true cypress open",
    "e2e-record-fixtures": "cross-env NODE_ENV=development CYPRESS_RECORD_FIXTURES=true cypress open",
    "e2e-ci": "cross-env NODE_ENV=test CYPRESS_USE_FIXTURES=true cypress run --record",
    "pree2e-ci": "cross-env NDLA_ENVIRONMENT=mock NODE_ENV=production node build/server &",
    "format": "prettier 'src/**/*(*.js|*.jsx|*.ts|*.tsx)' --write",
    "format-check": "prettier 'src/**/*(*.js|*.jsx|*.ts|*.tsx)' --check",
    "lint": "yarn format-check && yarn lint-es && yarn lint-css",
    "lint-css": "stylelint style/**/*.css",
    "lint-es": "eslint  --max-warnings=0 --cache --cache-location '.eslintcache/' --ext .js --ext .jsx src",
    "now-start": "NODE_ENV=production NOW=true node build/server",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider razzle start  --inspect --inspect-port 9230",
    "start-without-ssr": "cross-env NODE_OPTIONS=--openssl-legacy-provider DISABLE_SSR=true razzle start",
    "start-prod": "cross-env NODE_ENV=production node build/server",
    "test": "cross-env BABEL_ENV=test NODE_ENV=unittest jest",
    "test-ci": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --maxWorkers=4 --ci",
    "tdd": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --watch"
  },
  "engines": {
    "node": ">=20.13.1",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@ndla/scripts": "^2.1.2",
    "babel-preset-razzle": "^4.2.17",
    "chalk": "2.4.1",
    "cypress": "^7.3.0",
    "enzyme": "^3.8.0",
