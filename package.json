{
  "name": "learningpath-frontend",
  "version": "0.1.19",
  "description": "NDLA Learning Path Frontend",
  "scripts": {
    "pretest": "yarn run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "NODE_ENV=unittest jest",
    "tdd": "NODE_ENV=unittest jest --watch",
    "copy-svg-polyfill": "mkdir -p htdocs/assets && cp node_modules/svg4everybody/dist/svg4everybody.min.js htdocs/assets/",
    "prebuild": "yarn run clean && yarn run copy-svg-polyfill",
    "build": "NODE_ENV=production webpack --config ./webpack.config.prod.js --bail",
    "start": "NODE_ENV=development DISABLE_SSR=true nodemon --watch server server",
    "start-with-ssr": "NODE_ENV=development nodemon --watch server server",
    "start-prod": "NODE_ENV=production node server",
    "lint": "eslint src server"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "server",
      "node_modules"
    ],
    "testRegex": "(/__tests__/.*)(-test.js?)",
    "globals": {
      "__CLIENT__": false,
      "__SERVER__": true,
      "__DISABLE_SSR__": true
    }
  },
  "engines": {
    "node": "~6.2.2",
    "npm": "~3.9.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "0.28.0",
    "enzyme": "^2.8.2",
    "eslint-config-ndla": "0.0.7",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flux-standard-action": "1.2.0",
    "jest": "^19.0.2",
    "json-loader": "0.5.4",
    "nock": "9.0.6",
    "nodemon": "1.11.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "style-loader": "0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "dependencies": {
    "auth0-js": "^8.7.0",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "btoa": "1.1.2",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "date-fns": "^1.28.3",
    "defined": "1.0.0",
    "draft-convert": "^1.4.5",
    "draft-js": "^0.10.0",
    "eslint": "^3.13.1",
    "express": "4.15.2",
    "express-request-proxy": "2.0.0",
    "form-data": "^2.1.4",
    "helmet": "^3.6.1",
    "history": "4.6.1",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "4.17.4",
    "ndla-error-reporter": "^0.1.2",
    "ndla-learningpath-styleguide": "^0.2.0",
    "ndla-licenses": "0.0.7",
    "ndla-ui": "^0.3.2",
    "ndla-util": "^0.1.0",
    "node-polyglot": "2.2.2",
    "prop-types": "^15.5.8",
