    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "flux-standard-action": "^1.2.0",
    "jsdom": "^9.12.0",
    "json-loader": "0.5.4",
    "nock": "9.0.6",
    "nodemon": "1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "style-loader": "0.13.1",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "watch": "^1.0.2",
