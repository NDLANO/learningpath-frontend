{
  "name": "learningpath-frontend",
  "version": "0.1.19",
  "description": "NDLA Learning Path Frontend",
  "scripts": {
    "pretest": "yarn run lint",
    "test": "cross-env BABEL_ENV=test NODE_ENV=unittest jest",
    "test-ci": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --maxWorkers=4 --ci",
    "tdd": "cross-env BABEL_ENV=test NODE_ENV=unittest jest --watch",
    "copy-svg-polyfill": "mkdir -p htdocs/assets && cp node_modules/svg4everybody/dist/svg4everybody.min.js build/public/",
    "postbuild": "cp build/assets.json build/public/assets.json",
    "build": "razzle build",
    "start": "razzle start",
    "start-without-ssr": "cross-env DISABLE_SSR=true razzle start",
    "start-prod": "cross-env NODE_ENV=production node build/server",
    "e2e": "cypress open",
    "e2e-use-fixtures": "cross-env CYPRESS_USE_FIXTURES=true cypress open",
    "e2e-record-fixtures": "cross-env CYPRESS_RECORD_FIXTURES=true cypress open",
    "pree2e-ci": "cross-env NDLA_ENVIRONMENT=mock NODE_ENV=production node build/server &",
    "e2e-ci": "cross-env CYPRESS_USE_FIXTURES=true cypress run --record",
    "lint": "yarn lint-css && yarn lint-es && yarn lint-prettier",
    "lint-es": "eslint --cache --cache-location '.eslintcache/' --ext .js --ext .jsx src",
    "lint-css": "stylelint style/**/*.css",
    "lint-prettier": "node prettier.js lint",
    "prettier": "node prettier.js write"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "chalk": "2.4.1",
    "cross-env": "5.2.0",
    "cypress": "3.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-ndla": "0.5.1",
    "eslint-plugin-react": "7.10.0",
    "flux-standard-action": "2.0.3",
    "json-loader": "0.5.7",
    "nock": "9.4.4",
    "nodemon": "1.18.3",
    "postcss-cssnext": "3.1.0",
    "postcss-easy-import": "3.0.0",
    "postcss-focus": "3.0.0",
    "postcss-loader": "2.1.6",
    "postcss-nested": "3.0.0",
    "prettier": "1.14.0",
    "razzle-config-utils": "0.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.4",
    "stylelint": "9.4.0",
    "stylelint-config-standard": "18.2.0",
    "webpack-bundle-analyzer": "2.13.1"
  },
  "dependencies": {
    "auth0-js": "9.7.3",
