    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-sortable-hoc": "^0.6.3",
    "react-widgets": "^3.4.6",
    "redux": "3.6.0",
    "redux-actions": "0.11.0",
    "redux-form": "^6.6.3",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "2.2.0",
    "reselect": "2.5.4",
