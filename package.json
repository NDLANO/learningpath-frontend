{
  "name": "learningpath-frontend",
  "version": "0.1.16",
  "description": "NDLA Learning Path Frontend",
  "scripts": {
    "pretest": "npm run lint",
    "clean": "rimraf ./htdocs/assets/*",
    "test": "NODE_ENV=unittest babel-tape-runner 'src/**/*-test.js'|tap-spec",
    "tdd": "watch 'NODE_ENV=unittest babel-tape-runner 'src/**/*-test.js'|tap-spec||true' src/",
    "copy-svg-polyfill": "cp node_modules/svg4everybody/dist/svg4everybody.min.js htdocs/assets/",
    "prebuild": "npm run clean && npm run copy-svg-polyfill",
    "build": "NODE_ENV=production webpack --progress --bail",
    "start": "NODE_ENV=development nodemon --watch server server/server.js",
    "start-prod": "NODE_ENV=production node server/server.js",
    "start-dev-server": "webpack-dev-server --inline",
    "lint": "eslint src server"
  },
  "engines": {
    "node": "~6.2.2",
    "npm": "~3.9.5"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-tape-runner": "2.0.1",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "flux-standard-action": "0.6.1",
    "if-loader": "1.0.1",
    "jsdom": "9.4.2",
    "nock": "8.0.0",
    "nodemon": "1.10.2",
    "react-addons-test-utils": "15.3.1",
    "redux-mock-store": "1.1.4",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "tap-spec": "4.1.1",
    "tape": "4.6.0",
    "watch": "0.19.2",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-manifest-plugin": "1.0.1"
  },
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "babel-register": "6.11.6",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "date-fns": "1.3.0",
    "defined": "1.0.0",
    "draft-js": "0.8.1",
    "draft-js-export-html": "0.4.0",
    "draft-js-import-html": "0.2.0",
    "express": "4.14.0",
    "history": "2.1.2",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "json-loader": "0.5.4",
    "lodash": "4.15.0",
    "ndla-styleguide": "0.1.25",
    "node-polyglot": "2.0.0",
    "node-uuid": "1.4.7",
    "query-string": "4.2.3",
    "react": "15.3.1",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dom": "15.3.1",
